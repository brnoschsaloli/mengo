` Test file for comparisons and control flow
set a 10
set b 5
` Equal comparison
print 'a = 10, b = 5'
if a equal 10
    print 'a is equal to 10'
` Not equal comparison
if a notequal b
    print 'a is not equal to b'
` Less than comparison
if b less a
    print 'b is less than a'
` Greater than comparison
if a greater b
    print 'a is greater than b'
` While loop
print 'loop: i<5'
set i 0
while i less 5
    print i
    add i 1 i
exit 
