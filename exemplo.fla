input n
list numeros
set i 0
label ler_numeros
if i less n
    input temp
    insert temp numeros
    add i 1 i
    goto ler_numeros
set j 0
label verificar_lista
if j less n
    get j numeros atual
    print atual
    mod resto atual 2
    add j 1 j
    if resto equal 0
        print "par"
    if resto equal 0
        print "impar"
    goto verificar_lista
exit
